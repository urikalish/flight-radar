{
  "name": "flight-radar",
  "version": "1.0.0",
  "description": "Flight Radar",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "dev:server": "nodemon",
    "dev:client": "rollup -c -w",
    "build": "yarn clean && yarn build:server && yarn build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "rollup -c rollup.config.mjs",
    "start": "node dist/index.js",
    "clean": "rimraf dist dist-client",
    "lint": "eslint . --ext .ts,.js,.mjs",
    "lint:fix": "eslint . --ext .ts,.js,.mjs --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,css,html}\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "test": "echo \"No tests configured yet\"",
    "precommit": "yarn lint && yarn format:check && yarn typecheck",
    "full": "yarn format && yarn lint && yarn build"
  },
  "keywords": [
    "typescript",
    "express",
    "rollup",
    "fullstack"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.10",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "concurrently": "^8.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^15.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "typescript": "^5.5.4"
  }
}
